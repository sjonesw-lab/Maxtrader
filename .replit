packages = ["cairo", "ffmpeg-full", "freetype", "ghostscript", "glibcLocales", "gobject-introspection", "gtk3", "libyaml", "pkg-config", "qhull", "tcl", "tk"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "backtest-wave"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "ict-3day-test"

[[workflows.workflow]]
name = "backtest-wave"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python main_backtest_wave.py"

